{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@modules/*": ["modules/*"],
      "@models/*": ["models/*"]
    },

    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
